{
  "name": "supermall",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "better-scroll": "^2.3.1",
    "core-js": "^2.6.5",
    "fastclick": "^1.0.6",
    "vue": "^2.6.13",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.0.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "postcss-px-to-viewport": "^1.1.1",
    "vue-template-compiler": "^2.6.13"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-px-to-viewport": {
        "viewportWidth": 375,
        "viewportHeight": 667,
        "unitPrecision": 5,
        "propList": [
          "*"
        ],
        "viewportUnit": "vw",
        "fontViewportUnit": "vw",
        "selectorBlackList": [
          "ignore",
          "tabBar",
          "tabBarItem"
        ],
        "minPixelValue": 1,
        "mediaQuery": false,
        "replace": true,
        "landscape": false
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
